{
    "mode":"modules",
    "out":"doc",
    "theme":"default",
    "ignoreCompilerErrors": "true",
    "experimentalDecorators": "false",
    "emitDecoratorMetadata": "true",
    "target": "ES5",
    "moduleResolution": "node",
    "preserveConstEnums": "true",
    "stripInternal": "true",
    "suppressExcessPropertyErrors": "true",
    "suppressImplicitAnyIndexErrors": "true",
    "module": "commonjs"
}